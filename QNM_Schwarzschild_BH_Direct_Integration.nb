   3.7403329839609547`*^9, {3.740416234820445*^9, 3.7404162349033937`*^9}, {
   3.740416868956278*^9, 
   3.740416868994255*^9}},ExpressionUUID->"016ba5cf-2430-4d86-a275-\
f4d6a3fcaddc"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Psi]", "[", "r_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{"I", " ", "\[Omega]", " ", "r"}], "]"}], " ", 
    RowBox[{"g", "[", "r", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "r_", "]"}], " ", ":=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"ginf", "[", "i", "]"}], 
      SuperscriptBox["r", "i"]], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0", ",", 
       RowBox[{"ORDINF", "+", "5"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EQINF", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"g", "''"}], "[", "r", "]"}], " ", 
     SuperscriptBox[
      RowBox[{"f", "[", "r", "]"}], "2"]}], "+", 
    RowBox[{
     RowBox[{
      RowBox[{"g", "'"}], "[", "r", "]"}], " ", 
     RowBox[{
      RowBox[{"f", "'"}], "[", "r", "]"}], 
     RowBox[{"f", "[", "r", "]"}]}], "+", 
    RowBox[{"2", " ", "I", " ", "\[Omega]", " ", 
     RowBox[{"f", "[", "r", "]"}], " ", 
     RowBox[{
      RowBox[{"g", "'"}], "[", "r", "]"}]}], "-", 
    RowBox[{"V", " ", 
     RowBox[{"g", "[", "r", "]"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.740332786024994*^9, 3.7403329047766848`*^9}, {
   3.7403329467217627`*^9, 3.7403329469804664`*^9}, 
