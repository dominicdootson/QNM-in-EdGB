Cell[CellGroupData[{

Cell["Integration ", "Chapter",
 CellChangeTimes->{{3.7403330764590693`*^9, 
  3.740333082963587*^9}},ExpressionUUID->"25f7b008-8c91-4c0e-a0b1-\
a796ea95d76f"],

Cell[TextData[{
 "We now perform two different integrations. Firstly we integrate from the \
horizon to a matching point, r = rm, imposing purely ingoing wave boundary \
condition and secondly from infinity to the matching point while imposing no \
incoming waves at infinity.\n\nWe then construct a linear combination of the \
two solutions and fix the ration of the coefficients in order for the linear \
combination to be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["C", "0"], " "}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bcc6d2e1-3a47-46f2-8c2a-dabc5edd204f"],
 "at the matching point. Note for a generic frequency the linear combination \
will have discontinuous derivatives. Finally we find the eigenvalues by \
imposing that the linear combination is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["C", "1"], "."}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2f1e9629-4838-4638-abae-32cfe6aee168"],
 "\n\nWe first define the function that does the integral."
}], "Text",
 CellChangeTimes->{{3.740333088461701*^9, 3.74033335666928*^9}, {
  3.7403341373288946`*^9, 
  3.740334150628503*^9}},ExpressionUUID->"4e75f605-b87d-4e78-9e30-\
054f98839b43"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"INT", "[", 
    RowBox[{
    "\[Omega]0_", ",", "rm0_", ",", "l0_", ",", "EPS0_", ",", "INF0_"}], 
    "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ruleP", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"AccuracyGoal", "\[Rule]", "12"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "12"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"param", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"M", "\[Rule]", "1"}], ",", 
        RowBox[{"l", "\[Rule]", "l0"}], ",", 
        RowBox[{"\[Omega]", "\[Rule]", "\[Omega]0"}], ",", 
        RowBox[{
         RowBox[{"hh", "[", "0", "]"}], "\[Rule]", "1"}], ",", 
        RowBox[{
         RowBox[{"ginf", "[", "0", "]"}], "\[Rule]", "1"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rh", "=", 
      RowBox[{
       RowBox[{"2", "M"}], "/.", "param"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"EPS", "=", "EPS0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"rinf", "=", 
      RowBox[{
       RowBox[{"INF0", " ", "M"}], "/.", "param"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rhn", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "EPS"}], ")"}], "rh"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"**", 
       RowBox[{
       "*", "INTEGRATION", " ", "FROM", " ", "THE", " ", "HORIZON"}]}], 
      "***)"}], "\[IndentingNewLine]", 
     RowBox[{"\[Psi]gH", "=", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"r", "-", 
           RowBox[{"2", "M"}]}], ")"}], 
         RowBox[{
          RowBox[{"-", "2"}], " ", "I", " ", "\[Omega]", " ", "M"}]], 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"hh", "[", "i", "]"}], " ", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"r", "-", 
              RowBox[{"2", "M"}]}], ")"}], "i"]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", 
            RowBox[{"ORDH", "+", "1"}]}], "}"}]}], "]"}]}], "//.", 
       RowBox[{"Union", "[", 
        RowBox[{"param", ",", "seriesH"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"BCs", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"\[Psi]", "[", "rhn", "]"}], "\[Equal]", "\[Psi]gH"}], "/.", 
