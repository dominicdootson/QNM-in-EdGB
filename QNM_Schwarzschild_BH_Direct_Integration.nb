Cell["Next we write one really big for loop", "Text",
 CellChangeTimes->{{3.7404063362529593`*^9, 
  3.740406354908078*^9}},ExpressionUUID->"f395102c-cda6-40d3-ad69-\
a880ed94c6af"],

Cell[BoxData[
 RowBox[{
  RowBox[{"predictedModes", " ", "=", " ", 
   RowBox[{"Array", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"numbOvertones", ",", "4"}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.74040676349896*^9, 
  3.7404068390921173`*^9}},ExpressionUUID->"4e392e88-3f1e-4e46-bc15-\
3fff240c17e2"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"i", "=", "1"}], ",", " ", 
     RowBox[{"i", "<", "numbOvertones"}], ",", 
     RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"f4", "[", 
        RowBox[{"y_", "?", "NumericQ"}], "]"}], ":=", 
       RowBox[{
        RowBox[{"INT", "[", 
         RowBox[{"y", ",", "6", ",", "2", ",", 
          SuperscriptBox["10", 
           RowBox[{"-", "4"}]], ",", 
          RowBox[{"2", " ", 
           SuperscriptBox["10", "1"]}]}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"y0n", "=", 
       RowBox[{"y", "/.", 
        RowBox[{
         RowBox[{"FindRoot", "[", 
          RowBox[{
           RowBox[{"f4", "[", "y", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"y", ",", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"0.5", "*", 
                RowBox[{"initalGuess", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "1"}], "]"}], "]"}]}], " ", "-", " ", 
               RowBox[{"0.5", "*", 
                RowBox[{"initalGuess", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "2"}], "]"}], "]"}], "\[ImaginaryI]"}]}], 
              ")"}]}], "}"}], ",", " ", 
           RowBox[{"AccuracyGoal", "\[Rule]", "11"}]}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"check", " ", "=", " ", 
       RowBox[{"Abs", "[", 
        RowBox[{"f4", "[", "y0n", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"predictedModes", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Re", "[", "y0n", "]"}], ",", " ", 
         RowBox[{"-", 
          RowBox[{"Im", "[", "y0n", "]"}]}], ",", "check", ",", " ", 
         RowBox[{"i", "-", "1"}]}], "}"}]}]}]}], " ", "]"}], ";"}]}]], "Input",
 CellChangeTimes->{{3.740406400870625*^9, 3.740406407136607*^9}, {
