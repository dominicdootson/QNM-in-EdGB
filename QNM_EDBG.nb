         RowBox[{"{", 
          RowBox[{"r", ",", "RHo", ",", "rm0"}], "}"}], ",", "ruleP1", ",", 
         RowBox[{"Method", "->", "\"\<StiffnessSwitching\>\""}]}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[CapitalPsi]solh", "=", 
