          RowBox[{"(", 
           RowBox[{"l0", "+", "2"}], ")"}]}]}], ",", 
        RowBox[{"L", "\[Rule]", "l0"}], ",", 
        RowBox[{"\[Sigma]", "\[Rule]", "\[Sigma]0"}], ",", 
        RowBox[{"\[Alpha]", "\[Rule]", "\[Alpha]0"}], ",", 
        RowBox[{"M", "\[Rule]", "1"}], ",", 
        RowBox[{
         SubscriptBox["kpi", "0"], "\[Rule]", "e1i"}], ",", 
        RowBox[{
         SubscriptBox["f1i", "0"], "\[Rule]", "e2i"}], ",", 
        RowBox[{
         SubscriptBox["kpi", "0"], "\[Rule]", "e1h"}], ",", 
        RowBox[{
         SubscriptBox["f1i", "0"], "\[Rule]", " ", "e2h"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"from", " ", "the", " ", "horizon"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"EQS", "=", 
      RowBox[{"EQSs", "/.", "param"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"RHo", "=", 
      RowBox[{
       RowBox[{"2", "\[Epsilon]h0"}], "/.", "param"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ruexp", "=", 
      RowBox[{
       RowBox[{"rucoen", "//.", "param"}], "//", "Simplify"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"BOU", "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"bounno", "/.", 
          RowBox[{"r", "\[Rule]", 
           RowBox[{"(", "RHo", ")"}]}]}], "/.", "param"}], "//.", "ruexp"}], "//",
        "Simplify"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"solsy", "=", 
      RowBox[{
       RowBox[{"NDSolve", "[", 
        RowBox[{
         RowBox[{"Union", "[", 
          RowBox[{"EQS", ",", "BOU"}], "]"}], ",", 
