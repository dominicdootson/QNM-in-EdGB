         RowBox[{"H1", "[", "r", "]"}], ",", 
         RowBox[{"H0", "[", "r", "]"}], ",", 
         RowBox[{"\[Phi]1", "[", "r", "]"}], ",", 
         RowBox[{
          RowBox[{"\[Phi]1", "'"}], "[", "r", "]"}]}], "}"}], "/.", 
       "solsy"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"d\[CapitalPsi]solh", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"Kp", "'"}], "[", "r", "]"}], ",", 
          RowBox[{
           RowBox[{"H1", "'"}], "[", "r", "]"}], ",", 
          RowBox[{
           RowBox[{"H0", "'"}], "[", "r", "]"}], ",", 
          RowBox[{
           RowBox[{"\[Phi]1", "'"}], "[", "r", "]"}]}], "}"}], "/.", 
