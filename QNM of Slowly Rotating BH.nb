Cell[CellGroupData[{

Cell["Integration ", "Chapter",
 CellChangeTimes->{{3.7432072841722755`*^9, 
  3.7432072856316767`*^9}},ExpressionUUID->"ee6c64d1-9f66-48e1-931f-\
78860632761e"],

Cell[TextData[{
 "We now perform two different integrations. Firstly we integrate from the \
horizon to a matching point, r = rm, imposing purely ingoing wave boundary \
condition and secondly from infinity to the matching point while imposing no \
incoming waves at infinity.\n\nWe then construct a linear combination of the \
two solutions and fix the ration of the coefficients in order for the linear \
combination to be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["C", "0"], " "}], TraditionalForm]],ExpressionUUID->
  "51961ae9-de52-4053-b89a-2dcd8ee312fd"],
 "at the matching point. Note for a generic frequency the linear combination \
will have discontinuous derivatives. Finally we find the eigenvalues by \
imposing that the linear combination is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["C", "1"], "."}], TraditionalForm]],ExpressionUUID->
  "dabe0fe6-90a4-486e-95bf-cd450c63b932"],
 "\n\nWe first define the function that does the integral (note the above \
sections must be run first),"
}], "Text",
 CellChangeTimes->{
  3.743207292996936*^9, {3.7432681051651864`*^9, 
   3.743268117084869*^9}},ExpressionUUID->"4c8d4892-43ce-4372-91e8-\
83bc896e9785"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"INT", "[", 
    RowBox[{
    "\[Omega]0_", ",", "rm0_", ",", "l0_", ",", "EPS0_", ",", "INF0_", ",", 
     " ", "a0_", ",", " ", "m0_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Variables", " ", "of", " ", "the", " ", 
        RowBox[{"funciton", " ", "'"}], 
        RowBox[{"INT", "'"}], "\[IndentingNewLine]", "\[Omega]0"}], " ", "-", 
       " ", 
       RowBox[{
       "Inital", " ", "guess", " ", "of", " ", "the", " ", "QNM", 
        "\[IndentingNewLine]", "rm0"}], " ", "-", " ", 
       RowBox[{"Matching", " ", "point", "\[IndentingNewLine]", "l0"}], " ", 
       "-", " ", 
       RowBox[{"value", " ", "of", " ", "l", " ", 
        RowBox[{"(", 
         RowBox[{"harmonic", " ", "index"}], ")"}], "\[IndentingNewLine]", 
        "EPS0"}], " ", "-", " ", 
       RowBox[{"?", "\[IndentingNewLine]", 
        RowBox[{"INFO", " ", "-", " ", 
         RowBox[{
         "Value", " ", "that", " ", "infitely", " ", "far", " ", "away", " ", 
          "is", " ", "at", "\[IndentingNewLine]", "a0"}], " ", "-", " ", 
         RowBox[{
         "value", " ", "of", " ", "rotation", " ", "parameter", 
          "\[IndentingNewLine]", "m0"}], " ", "-", " ", 
         RowBox[{
         "value", " ", "of", " ", "m", " ", "spherical", " ", "harmonic", " ",
           "index"}]}]}]}], ",", " ", 
      RowBox[{"MUST", " ", "BE"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ruleP", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"AccuracyGoal", "\[Rule]", "12"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "12"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"param", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"M", "\[Rule]", "1"}], ",", 
        RowBox[{"l", "\[Rule]", "l0"}], ",", 
        RowBox[{"\[Omega]", "\[Rule]", "\[Omega]0"}], ",", 
        RowBox[{
         RowBox[{"hh", "[", "0", "]"}], "\[Rule]", "1"}], ",", 
        RowBox[{
         RowBox[{"ginf", "[", "0", "]"}], "\[Rule]", "1"}], ",", " ", 
        RowBox[{"a", "\[Rule]", " ", "a0"}], ",", " ", 
        RowBox[{"m", "\[Rule]", " ", "m0"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rh", "=", 
      RowBox[{
       RowBox[{"2", "M"}], "/.", "param"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"EPS", "=", "EPS0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"rinf", "=", 
      RowBox[{
       RowBox[{"INF0", " ", "M"}], "/.", "param"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rhn", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "EPS"}], ")"}], "rh"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"**", 
       RowBox[{
       "*", "INTEGRATION", " ", "FROM", " ", "THE", " ", "HORIZON"}]}], 
      "***)"}], "\[IndentingNewLine]", 
     RowBox[{"rs", "=", 
      RowBox[{
       RowBox[{"r", "+", 
        RowBox[{"2", "M", " ", 
         RowBox[{"Log", "[", 
          RowBox[{
           FractionBox["r", 
            RowBox[{"2", "M"}]], "-", "1"}], "]"}]}]}], "/.", "param"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"\[Psi]gH", "=", 
      RowBox[{"(*", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "I"}], " ", 
         RowBox[{"(", 
          RowBox[{"\[Omega]", "-", 
           FractionBox[
            RowBox[{"m", " ", "a"}], 
            RowBox[{"4", " ", 
             SuperscriptBox["M", "2"]}]]}], ")"}], " ", "rs"}], "]"}], "*)"}],
       " ", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"r", "-", 
           RowBox[{"2", "M"}]}], ")"}], 
         RowBox[{
          RowBox[{"-", "2"}], " ", "I", 
          RowBox[{"(", " ", 
           RowBox[{"\[Omega]", "-", 
            FractionBox[
             RowBox[{"m", " ", "a"}], 
             RowBox[{"4", " ", 
              SuperscriptBox["M", "2"]}]]}], ")"}], "  ", "M"}]], 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"hh", "[", "i", "]"}], " ", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"r", "-", 
              RowBox[{"2", "M"}]}], ")"}], "i"]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", 
            RowBox[{"ORDH", "+", "1"}]}], "}"}]}], "]"}]}], "//.", 
       RowBox[{"Union", "[", 
        RowBox[{"param", ",", "seriesH"}], "]"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Here", " ", "we", " ", "define", " ", "the", " ", "boundary", " ", 
       "conditions"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"BCs", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"\[Psi]", "[", "rhn", "]"}], "\[Equal]", "\[Psi]gH"}], "/.", 
         RowBox[{"r", "\[Rule]", "rhn"}]}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"\[Psi]", "'"}], "[", "rhn", "]"}], "\[Equal]", 
          RowBox[{"D", "[", 
           RowBox[{"\[Psi]gH", ",", "r"}], "]"}]}], "/.", 
         RowBox[{"r", "\[Rule]", "rhn"}]}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"EQs", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"-", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"-", 
                   FractionBox[
                    RowBox[{"6", " ", "M"}], 
                    SuperscriptBox["r", "3"]]}], "+", 
                  FractionBox[
                   RowBox[{"l", " ", 
                    RowBox[{"(", 
                    RowBox[{"1", "+", "l"}], ")"}]}], 
                   SuperscriptBox["r", "2"]]}], ")"}]}], " ", 
               RowBox[{"(", 
                RowBox[{"1", "-", 
                 FractionBox[
                  RowBox[{"2", " ", "M"}], "r"]}], ")"}]}], "+", 
              SuperscriptBox["\[Omega]", "2"]}], ")"}], " ", 
            RowBox[{"\[Psi]", "[", "r", "]"}]}], "+", 
           FractionBox[
            RowBox[{"2", " ", "M", " ", 
             RowBox[{"(", 
              RowBox[{"1", "-", 
               FractionBox[
                RowBox[{"2", " ", "M"}], "r"]}], ")"}], " ", 
             RowBox[{
              SuperscriptBox["\[Psi]", "\[Prime]",
               MultilineFunction->None], "[", "r", "]"}]}], 
            SuperscriptBox["r", "2"]], "+", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"1", "-", 
               FractionBox[
                RowBox[{"2", " ", "M"}], "r"]}], ")"}], "2"], " ", 
            RowBox[{
             SuperscriptBox["\[Psi]", "\[Prime]\[Prime]",
              MultilineFunction->None], "[", "r", "]"}]}]}], ")"}], 
         "\[Equal]", "0"}], "}"}], "/.", "param"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rm", "=", "rm0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"solH", "=", 
      RowBox[{"NDSolve", "[", 
       RowBox[{
        RowBox[{"Union", "[", 
         RowBox[{"EQs", ",", "BCs"}], "]"}], ",", "\[Psi]", ",", 
        RowBox[{"{", 
         RowBox[{"r", ",", "rhn", ",", "rm"}], "}"}], ",", "ruleP"}], "]"}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"\[Psi]nH", "=", 
      RowBox[{
       RowBox[{"\[Psi]", "[", "r", "]"}], "/.", 
       RowBox[{"solH", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Psi]nHp", "=", 
      RowBox[{"D", "[", 
       RowBox[{"\[Psi]nH", ",", "r"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", "******)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"**", "INTEGRATION"}], " ", "FROM", " ", "INFINITY"}], 
      "****)"}], "\[IndentingNewLine]", 
     RowBox[{"rs", "=", 
      RowBox[{
       RowBox[{"r", "+", 
        RowBox[{"2", "M", " ", 
         RowBox[{"Log", "[", 
          RowBox[{
           FractionBox["r", 
            RowBox[{"2", "M"}]], "-", "1"}], "]"}]}]}], "/.", "param"}]}], 
     ";", 
     RowBox[{"(*", 
      RowBox[{"tortoise", " ", "coordinate"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"\[Psi]gINF", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{"I", " ", "\[Omega]", " ", "rs"}], "]"}], 
        RowBox[{"Sum", "[", 
         RowBox[{
          FractionBox[
           RowBox[{"ginf", "[", "i", "]"}], 
           SuperscriptBox["r", "i"]], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", 
            RowBox[{"ORDINF", "-", "1"}]}], "}"}]}], "]"}]}], "//.", 
       RowBox[{"Union", "[", 
        RowBox[{"param", ",", "seriesINF"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"BCs", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"\[Psi]", "[", "rinf", "]"}], "\[Equal]", "\[Psi]gINF"}], "/.", 
         RowBox[{"r", "\[Rule]", "rinf"}]}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"\[Psi]", "'"}], "[", "rinf", "]"}], "\[Equal]", 
          RowBox[{"D", "[", 
           RowBox[{"\[Psi]gINF", ",", "r"}], "]"}]}], "/.", 
